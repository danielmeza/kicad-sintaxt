{
    "comments": {
        // KiCad uses semicolons for comments in s-expression files
        "lineComment": ";"
    },
    // symbols used as brackets
    "brackets": [
        ["(", ")"]
    ],
    // symbols that are auto closed when typing
    "autoClosingPairs": [
        ["(", ")"],
        ["\"", "\""]
    ],
    // symbols that can be used to surround a selection
    "surroundingPairs": [
        ["(", ")"],
        ["\"", "\""]
    ]
}